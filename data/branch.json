    "command": "git checkout (nom-branche)"
  },
  {
    "def": "Renommer la branche courante",
    "command": "git branch -m (nouveau-nom)"
  },
  {
    "def": "Supprimer une branche en local",
    "command": "git branch -d (nom-branche)"
  },
  {
    "def": "Supprimer une branche en local peut importe si elle a été mergé ou pas",
    "command": "git branch -D (nom-branche)"
  }
